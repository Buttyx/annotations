{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "../../../js",
            "../../../js/models",
            "../../../js/collections",
            "../../../js/views",
            "../../../js/prototypes"
        ],
        "exclude": ["../../../js/libs"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "../../../docs",
        "recurse": false
    },
    "jsVersion": 180
}